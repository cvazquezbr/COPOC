{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/((?!api/|assets/|.*\\.).*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/transcribe.js": {
      "maxDuration": 60
    }
  }
}
